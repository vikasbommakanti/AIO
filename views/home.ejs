  <main id="main">

    <!-- ======= About Us Section ======= -->
    <!-- ======= Recent Blog Posts Section ======= -->
    <section id="recent-posts" class="recent-posts sections-bg">
      <div class="container" data-aos="fade-up">

        <div class="section-header">
          <h2>Recent Blog Posts</h2>
          <p>One of the fundamental principles of time management is prioritization. Start by identifying your most important tasks or goals and focus on them first.</p>
        </div>

        <div class="row gy-4">

         
           

              <% posts.slice(posts.length-3,posts.length).reverse().forEach(post => { %>
                <div class="col-xl-4 col-md-6">
                <article>
                  <% if (post.image) { %>
                    <div class="post-img">
                      <img src="<%= post.image %>" alt="Post Image" class="img-fluid">
                    </div>
                  <% } %>
                  <p class="post-category"><%= post.title %></p>
                  <p class="title">
                    <p><%= post.body %></p>
                    <p>~ <%= post.namee %></p>
                  </p>
                  
              </article>
            </div>
              <% }) %>
           
          

        

        </div>

      </div>
    </section><!-- End Recent Blog Posts Section -->

    
            

  </main><!-- End #main -->

 

